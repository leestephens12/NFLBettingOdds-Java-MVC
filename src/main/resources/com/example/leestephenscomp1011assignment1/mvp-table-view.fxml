         <center>
            <TableView fx:id="mvpTableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="playerIdColumn" prefWidth="150.0" text="Player Id" />
